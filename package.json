{
  "name": "@useoptic/cli",
  "version": "0.1.0-alpha",
  "description": "Optic CLI",
  "engines": {
    "node": ">= 10.13 <11"
  },
  "files": [
    "/build"
  ],
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/path-to-regexp": "^1.7.0",
    "jest": "~23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "1.15.3",
    "rimraf": "^2.6.3",
    "ts-jest": "~23.10.5",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-microsoft-contrib": "~6.0.0",
    "tsutils": "~3.7.0",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "build:watch": "npm run clean && tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "echo:start": "npx nodemon build/src/echo-server.js",
    "proxy:start": "npx nodemon build/src/proxy-server.js",
    "logging:start": "npx nodemon build/src/logging-server.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "deepdash": "^1.9.5",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "http-proxy": "^1.17.0",
    "lodash": "^4.17.11",
    "path-match": "^1.2.4",
    "path-to-regexp": "^3.0.0",
    "tslib": "~1.9.3"
  }
}
